{
  "expo": {
    "name": "Aerodyn Mobile",
    "slug": "aerodyn_mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/adaptive-icon.png",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#000000",
    "newArchEnabled": true,
    "sdkVersion": "53.0.0",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aerodyn.mobile",
      "splash": {
        "image": "./assets/logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000",
        "dark": {
          "image": "./assets/logo.png",
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "googleMapsApiKey": "AIzaSyBas_EXXMv25PXDBvTBhB3c1ALO1fHhPMo"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "splash": {
        "image": "./assets/logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000",
        "dark": {
          "image": "./assets/logo.png",
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      },
      "edgeToEdgeEnabled": true,
      "package": "com.aerodyn.mobile",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBas_EXXMv25PXDBvTBhB3c1ALO1fHhPMo"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "splash": {
        "image": "./assets/logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000"
      }
    },
    "extra": {
      "eas": {
        "projectId": "fd2f877b-da72-4974-9b3a-c8aec71e91ce"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "networkSecurityConfig": true,
            "usesCleartextTraffic": true
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ]
  }
}

{
  "name": "aerodyn_mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf node_modules && npm install",
    "build:android": "eas build -p android --profile production",
    "build:ios": "eas build -p ios --profile production",
    "build:preview": "eas build -p android --profile preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "^8.3.0",
    "@react-native-community/slider": "4.5.6",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "axios": "^1.6.2",
    "expo": "~49.0.15",
    "expo-build-properties": "~0.14.6",
    "expo-constants": "^17.1.6",
    "expo-device": "^7.1.4",
    "expo-document-picker": "~11.5.4",
    "expo-file-system": "~15.4.5",
    "expo-image-picker": "~14.3.2",
    "expo-location": "~18.1.5",
    "expo-status-bar": "~1.6.0",
    "mobx": "^6.12.0",
    "mobx-react-lite": "^4.0.5",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-geocoding": "^0.5.0",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-maps": "1.7.1",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-paper": "^5.11.3",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-vector-icons": "^10.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-export-namespace-from": "^7.27.1",
    "@babel/runtime": "^7.27.4",
    "@types/react": "~18.2.14",
    "@types/react-native-vector-icons": "^6.4.18",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.54.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "metro": "^0.82.4",
    "metro-core": "^0.82.4",
    "metro-resolver": "^0.82.4",
    "metro-runtime": "^0.82.4",
    "metro-source-map": "^0.82.4",
    "typescript": "^5.1.3"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": ["react-native-geocoding"],
        "listUnknownPackages": false
      }
    }
  }
}
